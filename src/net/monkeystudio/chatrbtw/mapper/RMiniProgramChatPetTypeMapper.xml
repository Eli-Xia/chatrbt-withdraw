<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="net.monkeystudio.chatrbtw.mapper.RMiniProgramChatPetTypeMapper">
    <resultMap id="baseResultMap" type="net.monkeystudio.chatrbtw.entity.RMiniProgramChatPetType">
        <result column="id" property="id" jdbcType="INTEGER" />
        <result column="mini_program_id" property="miniProgramId" jdbcType="INTEGER" />
        <result column="chat_pet_type" property="chatPetType" jdbcType="INTEGER" />
    </resultMap>

    <sql id="Base_Column_List" >
        id, mini_program_id, chat_pet_type
    </sql>


    <select id="selectByWxMinProgramId" resultMap="baseResultMap" parameterType="map">
        select
            <include refid="Base_Column_List" />
        from
            r_mini_program_chat_pet_type
        where
            mini_program_id = #{wxMiniProgramId}
    </select>

</mapper>