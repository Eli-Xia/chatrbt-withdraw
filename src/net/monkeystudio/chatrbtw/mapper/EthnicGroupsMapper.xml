<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="net.monkeystudio.chatrbtw.mapper.EthnicGroupsMapper">
  <resultMap id="baseResultMap" type="net.monkeystudio.chatrbtw.entity.EthnicGroups">
    <result column="id" property="id" jdbcType="INTEGER" />
    <result column="total_valid_count" property="totalValidCount" jdbcType="INTEGER" />
    <result column="wx_pub_origin_id" property="wxPubOriginId" jdbcType="VARCHAR" />
    <result column="code_type" property="codeType" jdbcType="INTEGER" />
    <result column="second_founder_id" property="secondFounderId" jdbcType="INTEGER" />
    <!--<result column="code" property="code" jdbcType="BIGINT" />-->
    <!--<result column="parent_code" property="parentCode" jdbcType="BIGINT" />-->
    <result column="parent_id" property="parentId" jdbcType="INTEGER" />
    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
  </resultMap>

  <sql id="Base_Column_List" >
    id, total_valid_count, wx_pub_origin_id, code_type, second_founder_id, parent_id, create_time
  </sql>

  <select id="selectByWxPubAndSecondFunder" parameterType="map" resultMap="baseResultMap">
    select
      <include refid="Base_Column_List"></include>
    from
      e_ethnic_groups
    where
      wx_pub_origin_id = #{wxPubOriginId}

    <if test="secondFounderId == null">
      and `second_founder_id` is null
    </if>

    <if test="secondFounderId != null">
      and `second_founder_id` = #{secondFounderId}
    </if>
  </select>


  <!--<select id="countByCode" parameterType="map" resultType="java.lang.Integer">
    select
      count(id)
    from
      e_ethnic_groups
    where
      `code` = #{code}
    and
      parent_code is not null
  </select>-->

  <select id="selectFounderByWxPub" resultMap="baseResultMap" parameterType="map">
    select
      <include refid="Base_Column_List"></include>
    from
      e_ethnic_groups
    where
      wx_pub_origin_id = #{wxPubOriginId} and code_type = #{codeType}
  </select>


  <insert id="insert" parameterType="net.monkeystudio.chatrbtw.entity.EthnicGroups" keyProperty="id" useGeneratedKeys="true" >
    insert into
      e_ethnic_groups (
      total_valid_count,
      wx_pub_origin_id,
      code_type,
      second_founder_id,
      parent_id,
      create_time)
    values(
      #{totalValidCount},
      #{wxPubOriginId},
      #{codeType},
      #{secondFounderId},
      #{parentId},
      #{createTime}
    )
  </insert>
</mapper>
